function rba_model_elements = rba_define_simple_example_model()

% rba_model_elements = rba_get_simple_example_model()

rba_model_elements = struct;

rba_model_elements.elements.compartment.IDsymbol                  = numbered_names('cpt',1,0);
rba_model_elements.elements.localized_metabolite.IDsymbol         = numbered_names('met',1,0);
rba_model_elements.elements.localized_enzymatic_reaction.IDsymbol = numbered_names('rea',1,0);
rba_model_elements.elements.localized_enzyme.IDsymbol             = numbered_names('enz',1,0);
rba_model_elements.elements.localized_protein.IDsymbol            = numbered_names('prt',2,0);
rba_model_elements.elements.localized_molecular_process.IDsymbol  = numbered_names('prc',1,0);
rba_model_elements.elements.localized_molecular_machine.IDsymbol  = numbered_names('mch',1,0);
rba_model_elements.elements.physiological_target.IDsymbol         = numbered_names('tgt',1,0);

rba_model_elements.variables.compartment_volume.LowerBound        = 0 * ones(1,1);
rba_model_elements.variables.metabolite_concentration.LowerBound  = 0 * ones(1,1);
rba_model_elements.variables.metabolite_growth_balance.LowerBound = 0 * ones(1,1);
rba_model_elements.variables.reaction_flux.LowerBound             = 0 * ones(1,1);
rba_model_elements.variables.reaction_flux_max_fw.LowerBound      = 0 * ones(1,1);
rba_model_elements.variables.reaction_flux_max_rv.LowerBound      = 0 * ones(1,1);
rba_model_elements.variables.enzyme_concentration.LowerBound      = 0 * ones(1,1);
rba_model_elements.variables.protein_concentration.LowerBound     = 0 * ones(2,1);
rba_model_elements.variables.protein_growth_balance.LowerBound    = 0 * ones(2,1);
rba_model_elements.variables.process_flux.LowerBound              = 0 * ones(1,1);
rba_model_elements.variables.process_flux_max.LowerBound          = 0 * ones(1,1);
rba_model_elements.variables.machine_concentration.LowerBound     = 0 * ones(1,1);
rba_model_elements.variables.compartment_packing.LowerBound       = 0 * ones(1,1);
rba_model_elements.variables.compartment_packing_max.LowerBound   = 0 * ones(1,1);
rba_model_elements.variables.target_value.LowerBound              = 0 * ones(1,1);

rba_model_elements.variables.compartment_volume.UpperBound        = 1 * ones(1,1);
rba_model_elements.variables.metabolite_concentration.UpperBound  = 1 * ones(1,1);
rba_model_elements.variables.metabolite_growth_balance.UpperBound = 1 * ones(1,1);
rba_model_elements.variables.reaction_flux.UpperBound             = 1 * ones(1,1);
rba_model_elements.variables.reaction_flux_max_fw.UpperBound      = 1 * ones(1,1);
rba_model_elements.variables.reaction_flux_max_rv.UpperBound      = 1 * ones(1,1);
rba_model_elements.variables.enzyme_concentration.UpperBound      = 1 * ones(1,1);
rba_model_elements.variables.protein_concentration.UpperBound     = 1 * ones(2,1);
rba_model_elements.variables.protein_growth_balance.UpperBound    = 1 * ones(2,1);
rba_model_elements.variables.process_flux.UpperBound              = 1 * ones(1,1);
rba_model_elements.variables.process_flux_max.UpperBound          = 1 * ones(1,1);
rba_model_elements.variables.machine_concentration.UpperBound     = 1 * ones(1,1);
rba_model_elements.variables.compartment_packing.UpperBound       = 1 * ones(1,1);
rba_model_elements.variables.compartment_packing_max.UpperBound   = 1 * ones(1,1);
rba_model_elements.variables.target_value.UpperBound              = 1 * ones(1,1);

rba_model_elements.connections.growth_rate_Imet                    = eye(1);
rba_model_elements.connections.growth_rate_Iprt                    = eye(2);
rba_model_elements.connections.N_reaction_stoichiometry            = [1];
rba_model_elements.connections.N_metabolite_process_stoichiometry  = [-1];
rba_model_elements.connections.N_protein_process_stoichiometry     = [1; 1];
rba_model_elements.connections.R_enzyme_composition                = [1; 0];
rba_model_elements.connections.R_machine_composition               = [0; 1];
rba_model_elements.connections.E_enzyme_capacity_fw                = [1];
rba_model_elements.connections.E_enzyme_capacity_rv                = [1];
rba_model_elements.connections.E_machine_capacity                  = [1];
rba_model_elements.connections.D_enzyme_volume                     = 1;
rba_model_elements.connections.D_machine_volume                    = 1;
rba_model_elements.connections.L_target_metabolite                 = 0;
rba_model_elements.connections.L_target_reaction_flux              = 1;
rba_model_elements.connections.L_target_process_flux               = 0;
rba_model_elements.connections.L_target_protein                    = [0 0];
rba_model_elements.connections.L_target_machine                    = 0;
rba_model_elements.connections.Q_compartment_crowding              = [1];
