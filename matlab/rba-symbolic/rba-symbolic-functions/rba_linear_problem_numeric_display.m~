function rba_linear_problem_numeric_display(LP_numeric)

% display [A,a] as matrix
display('Matrix [A|a]')
[ {''}, LP_numeric.x_id', {'RHS'}; LP_numeric.a_id, num2cell(LP_numeric.A), num2cell(LP_numeric.a)]

% display [B,b] as matrix

display('Matrix [B|b]')
[ {''}, LP_numeric.x_id', {'RHS'}; LP_numeric.b_id, num2cell(LP_numeric.B), num2cell(LP_numeric.b)]
